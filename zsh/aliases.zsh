# Go back
alias ..="cd .."

# Git add
alias ga="git add -A"

# Git commit
alias gc="git commit"

# Git status
alias gs="git status"

# Git push
alias gp="git push"

# Git log
alias gl="git log --oneline"

# Git log
alias gd="git diff"

# Git checkout main
alias gcm="git checkout main"

# Git checkout new branch
alias gcn="git checkout -b"

# Django runserver
alias runserver="./manage.py runserver"

# Django make migrations
alias makemigrations="./manage.py makemigrations"

# Django migrate
alias migrate="./manage.py migrate"

# Shell pluss
alias shell="./manage.py shell_plus"

# Go to dotfiles
alias dotfiles="cd ~/code/dotfiles"

# Jump to tienda
alias tienda="cd ~/code/tienda"
alias web="cd ~/code/tienda-web"
alias hub="cd ~/code/hub"
alias camino="cd ~/code/camino"

# Klipy
alias klipy="~/code/klipy/.venv/bin/python -m klipy"